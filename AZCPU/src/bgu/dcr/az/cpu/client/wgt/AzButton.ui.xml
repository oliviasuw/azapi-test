<!DOCTYPE ui:UiBinder SYSTEM "http://dl.google.com/gwt/DTD/xhtml.ent">
<ui:UiBinder xmlns:ui="urn:ui:com.google.gwt.uibinder"
	xmlns:g="urn:import:com.google.gwt.user.client.ui">
	<ui:style>
		.button {
			color: rgb(220,220,220);
			vertical-align: middle;
			padding-top: 3px;
			padding-bottom: 3px;
			padding-right: 6px;
			padding-left: 6px;
			margin-right: 10px;
			float:left;
			background-color: rgb(51,51,51);
			border-color: black;
			border-style: groove;
		}
		
		.button .text {
			
		}
		
	</ui:style>
	<g:HTMLPanel>
		<button class="{style.button}" vertical-align="middle" ui:field="button">
			<img style="vertical-align: middle" src="images/algorithm.png"
				ui:field="image">
				<span class="{style.text}" ui:field="text"> BUTTON </span>
			</img>
		</button>
	</g:HTMLPanel>
</ui:UiBinder> 
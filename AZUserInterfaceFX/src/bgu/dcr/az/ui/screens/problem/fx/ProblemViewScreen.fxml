<BorderPane id="AnchorPane" fx:id="data" prefHeight="400.0" prefWidth="600.0" styleClass="az-root" xmlns:fx="http://javafx.com/fxml/1" xmlns="http://javafx.com/javafx/2.2" fx:controller="bgu.dcr.az.ui.screens.problem.fx.ProblemViewScreenCtl">
  <center>
    <SplitPane fx:id="split" dividerPositions="" focusTraversable="true" maxHeight="1.7976931348623157E308" maxWidth="1.7976931348623157E308" orientation="VERTICAL" prefHeight="-1.0" prefWidth="-1.0">

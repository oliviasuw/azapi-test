<?xml version="1.0" encoding="UTF-8"?>

<?import java.lang.*?>
<?import java.net.*?>
<?import javafx.collections.*?>
<?import javafx.scene.control.*?>
<?import javafx.scene.layout.*?>

<BorderPane id="AnchorPane" fx:id="container" prefHeight="400.0" prefWidth="600.0" styleClass="az-root" xmlns:fx="http://javafx.com/fxml/1" xmlns="http://javafx.com/javafx/2.2" fx:controller="bgu.dcr.az.ui.screens.problem.ProblemViewScreenCtl">
    <center>
        <SplitPane fx:id="split" dividerPositions="" focusTraversable="true" maxHeight="1.7976931348623157E308" maxWidth="1.7976931348623157E308" orientation="VERTICAL" prefHeight="-1.0" prefWidth="-1.0">
            <items>
                <BorderPane fx:id="data" maxHeight="1.7976931348623157E308" maxWidth="1.7976931348623157E308" minHeight="-Infinity" minWidth="-Infinity">
                    <center>
                        <TableView fx:id="table" maxHeight="-Infinity" maxWidth="-Infinity" prefHeight="-1.0" prefWidth="-1.0" styleClass="no-headers" visible="true" BorderPane.alignment="CENTER" />
                    </center>
                </BorderPane>
            </items>
        </SplitPane>
    </center>
    <left>
        <TreeView fx:id="tree" prefHeight="200.0" prefWidth="200.0" />
    </left>
    <stylesheets>
        <URL value="@../../azstyle.css" />
    </stylesheets>
    <top>
        <VBox fx:id="caption" maxHeight="1.7976931348623157E308" maxWidth="1.7976931348623157E308" prefHeight="-1.0" prefWidth="-1.0">
            <children>
                <FlowPane fx:id="bot" prefHeight="-1.0" prefWidth="-1.0">
                    <children>
                        <Label fx:id="problemViewingDescription" maxHeight="1.7976931348623157E308" maxWidth="1.7976931348623157E308" text="Viewing Problem X of Test Y" />
                        <Hyperlink fx:id="changeProblemHyperlink" maxHeight="1.7976931348623157E308" maxWidth="1.7976931348623157E308" text="change?" />
                    </children>
                </FlowPane>
                <FlowPane id="problemChooser" fx:id="top" prefHeight="-1.0" prefWidth="-1.0">
                    <children>
                        <Label minHeight="-1.0" minWidth="-1.0" text="Test" />
                        <ComboBox fx:id="testSelect" minHeight="-1.0" minWidth="-1.0">
                            <items>
                                <FXCollections fx:factory="observableArrayList">
                                    <String fx:value="Item 1" />
                                    <String fx:value="Item 2" />
                                    <String fx:value="Item 3" />
                                </FXCollections>
                            </items>
                        </ComboBox>
                        <Label minHeight="-1.0" minWidth="-1.0" text=", Problem Number" />
                        <TextField fx:id="pnumSelect" maxWidth="50.0" minHeight="-1.0" minWidth="50.0" prefWidth="50.0" />
                        <Button fx:id="viewProblemBtn" maxHeight="1.7976931348623157E308" maxWidth="1.7976931348623157E308" minHeight="-1.0" mnemonicParsing="false" text="View" />
                    </children>
                </FlowPane>
            </children>
        </VBox>
    </top>
</BorderPane>
